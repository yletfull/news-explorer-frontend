{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": [0 , { "allow": ["_id"] }],
    "linebreak-style": [0],
    "no-undef": [0],
    "prefer-promise-reject-errors": [0],
    "class-methods-use-this": [0],
    "no-restricted-globals": [0],
    "func-names": [0],
    "no-loop-func": [0],
    "no-restricted-syntax": [0],
    "no-plusplus": [0],
    "max-len": [0],
    "no-shadow": [0],
    "no-return-assign": [0],
    "no-param-reassign": [0]
  }

}